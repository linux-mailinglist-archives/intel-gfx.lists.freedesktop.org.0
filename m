> +		return 0;
> +
>  	GEM_TRACE("flags=3D%x (%s), timeout=3D%ld%s, awake?=3D%s\n",
>  		  flags, flags & I915_WAIT_LOCKED ? "locked" :
> "unlocked",
>  		  timeout, timeout =3D=3D MAX_SCHEDULE_TIMEOUT ? "
> (forever)" : "",
>  		  yesno(i915->gt.awake));
> =20
